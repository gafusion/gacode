#!/bin/bash

list="
 run.out
 t.out
 diff.out
 diff_i.out
 diff_n.out
 error.out
 gbflux.out
 gbflux_i.out
 gbflux_n.out
 geometry_arrays.out
 INPUT
 INPUT.gen
 input.profiles
 input.profiles.gen
 input.profiles.geo
 kxkyspec.out
 phase_space.out
 profile_vugyro.out
 timing.out
 VERSION_tag
 zerobar.out"

target="$1_s"
mkdir $target

for i in $list
do
   cp -v $1/$i $target
done
  
