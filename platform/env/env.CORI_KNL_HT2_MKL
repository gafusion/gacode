#!/bin/bash

if [ -n "$SSH_TTY" ] ; then
   echo "Setting up $GACODE_PLATFORM environment for gacode"
fi

# The default intel/18 compiler seems broken
module load nano

module swap intel/19.0.3.199 intel/18.0.3.222

module load python
module load cray-netcdf

# use mkl, not libsci
module unload cray-libsci
