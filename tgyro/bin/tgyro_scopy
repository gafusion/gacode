tgyrolist="
description.txt
gyrotest_flag
VERSION_tag
input.tgyro
input.tgyro.gen
input.tgyro.log
run.out"

tgyrotarget="$1_s"
mkdir $tgyrotarget

for i in $tgyrolist
do
   cp -v $1/$i $tgyrotarget
done

gyrodirs=`cat $1/input.tgyro | awk '/DIR/ { print $2 }'`

for i in $gyrodirs
do
   cd $1
   gyro_scopy $i
   cd ..
   mv "$1/${i}_s" "$tgyrotarget/$i"
done
