#!/usr/bin/env python
#
# SCRIPT:
#  tgyro_parse 
#
# PURPOSE:
#  Parse the top-level input file, TGYRO_INPUT.  This will also
#  recursively initialize/parse all GYRO simdirs.
#---------------------------------------------------------------

import sys
import tgyro_util

nproc   = sys.argv[1]
logfile = sys.argv[2]

mproc,error = tgyro_util.parse('INPUT_tgyro','INPUT_tgyro.gen',\
                               logfile,nproc)

sys.exit(error)
