#!/bin/sh
#
# SCRIPT:
#  tgyro_version_message
#
# FUNCTION:
#  Echo version message.
#------------------------------

if [ -d $GACODE_ROOT/.git ] 
then
   VERSION=`cd $GACODE_ROOT ; git describe --abbrev=4`
else
   VERSION=`cat $GACODE_ROOT/.VERSION` 
fi

echo "=========================================================="
echo "TGYRO" `echo $VERSION` `echo $GACODE_PLATFORM` `uname -s` `uname -m`
echo "=========================================================="
