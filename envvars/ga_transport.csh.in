setenv GACODE_ROOT @abs_top_builddir@
setenv GYRO_DIR @abs_top_builddir@/gyro
setenv NEO_DIR @abs_top_builddir@/neo
setenv TGLF_DIR @abs_top_builddir@/tglf
setenv PATH ${PATH}:$GACODE_ROOT/bin:$GYRO_DIR/bin:$NEO_DIR/bin:$TGLF_DIR/bin
setenv PYTHONADD $GACODE_ROOT/bin:$GACODE_ROOT/share:$GACODE_ROOT/shared/python:$GYRO_DIR/python:$NEO_DIR/python:$TGLF_DIR/python
#setenv IDL_PATH $IDL_DIR/lib:$GYRO_DIR/vugyro

if (${?PYTHONPATH}) then
  setenv PYTHONPATH "${PYTHONADD}:${PYTHONPATH}"
else
  setenv PYTHONPATH "$PYTHONADD"
endif

