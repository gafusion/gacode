include ${GACODE_ROOT}/shared/install/make.inc.${GACODE_PLATFORM}

LLIB  = xptor_lib
ARCH = ar cvs

OBJECTS = datread_lib.a \
          models_lib.a \
          nclmisc_lib.a \
          ptmisc_lib.a  \
          solver_lib.a  \
          source_lib.a 


.SUFFIXES : .a


lib :  $(OBJECTS) 
	ar rsv $(LLIB).a $(OBJECTS)

clean:
	rm -f $(LLIB).a

